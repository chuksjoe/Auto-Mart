{
	"parser": "babel-eslint",
	"extends": "airbnb-base",
	"parserOptions": {
		"ecmaVersion": 6
	},
	"env": {
		"node": true,
		"browser": true,
		"es6": true,
		"mocha": true
	},
	"rules": {
		"indent": [0, "tab"],
		"no-tabs": 0,
		"camelcase": 0,
		"linebreak-style": "off"
	}
}
